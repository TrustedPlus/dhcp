{
  "name": "dhcp-mon",
  "version": "1.0.3",
  "description": "NodeJS implementation of DHCP socket connection",
  "main": "out/index.js",
  "types": "index.d.ts",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "@types/node": "^10.5.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.4",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "build:map": "tsc --sourceMap",
    "build:source": "tsc --declaration --declarationDir types",
    "prepatch": "npm run build",
    "patch": "npm version patch",
    "pub": "npm run patch && npm publish && git push",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrustedPlus/dhcp-mon.git"
  },
  "keywords": [
    "dhcp",
    "net",
    "mac",
    "server",
    "client",
    "socket"
  ],
  "author": "",
  "contributors": [
    "Miroshin Stepan<microshine@mail.ru>",
    "Protasova Elena<lendjeina@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrustedPlus/dhcp-mon/issues"
  },
  "homepage": "https://github.com/TrustedPlus/dhcp-mon#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
